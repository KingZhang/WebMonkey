{
	"manifest_version": 2,
	"name": "webMonkey",
	"version": "1.0.0",
	"description": "web monkey",
	"icons":
	{
		"16": "images/logo.png",
		"48": "images/logo.png",
		"128": "images/logo.png"
	},
	"background":
	{
		"scripts": ["js/background.js"]
	},
	"browser_action": 
	{
		"default_icon": "images/logo.png",
		"default_title": "web monkey",
		"default_popup": "html/popup.html"
	},
	"content_scripts": 
	[
		{
			"matches": ["<all_urls>"],
			"js": ["js/gremlins.min.js", "js/content.js"],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": ["js/inner.js"],
	"permissions":
	[
		"contextMenus",
		"tabs",
		"notifications",
		"webRequest",
		"webRequestBlocking",
		"storage",
		"http://*/*",
		"https://*/*"
	],
	"homepage_url": "https://baidu.com",
	"options_ui":
	{
		"page": "html/option.html",
		"open_in_tab": true,
		"chrome_style": true
	},
	"omnibox": { "keyword" : "monkey" }
}